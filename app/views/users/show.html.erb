<h1>アカウント詳細ページ</h1>
<p>名前: <%= @user.name %></p>
<p>メールアドレス: <%= @user.email %></p>
<div>
<tbody>
  <tr>
  <td><%= link_to "編集", edit_user_path(@user), id:"edit-user" %></td> |
  <td><%= link_to '削除', @user, method: :delete, data: { confirm: '本当に削除してもよろしいですか？' }, id:"destroy-user" %></td>
  </tr>
 </tbody>

